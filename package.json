{
  "name": "element-plus-mo",
  "displayName": "Element Plus Mo",
  "description": "Code snippets for Element-Plus",
  "version": "0.0.7",
  "publisher": "momomomowei",
  "icon": "img/icon.png",
  "license": "MIT",
  "engines": {
    "vscode": "^1.72.0"
  },
  "scripts": {
    "pack": "vsce package",
    "build": "rollup -c --environment NODE_ENV:production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/momomomowei/element-plus-mo.git"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:vue",
    "onLanguage:html"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "snippets": [
      {
        "path": "./src/snippets/snippets.code-snippets"
      },
      {
        "path": "./src/snippets/element-ui.code-snippets"
      },
      {
        "path": "./src/snippets/vue.code-snippets"
      },
      {
        "path": "./src/snippets/vue-page.code-snippets"
      }
    ]
  },
  "dependencies": {
    "vscode-html-languageservice": "^5.5.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.41.1"
  }
}
